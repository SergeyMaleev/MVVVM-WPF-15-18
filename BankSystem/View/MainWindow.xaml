<Window
    x:Class="BankSystem.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:BankSystem"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="clr-namespace:BankSystem.ViewModels"
    Title="Банк"
    Width="960"
    Height="600"
    DataContext="{Binding Source={StaticResource vmLocator}, Path=MainWindowViewModel}"
    FontSize="16"
    mc:Ignorable="d"
    WindowStartupLocation="CenterScreen"
    ResizeMode="NoResize"
    >



    <Grid>
        <Frame Content="{Binding CurrenPage}" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
